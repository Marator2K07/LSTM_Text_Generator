        <widget class="QLabel" name="influenceOfBatchSizeHeaderLabel">
         <property name="font">
          <font>
           <family>Segoe UI Light</family>
           <pointsize>12</pointsize>
           <italic>true</italic>
          </font>
         </property>
         <property name="text">
          <string>Влияние размера партии:</string>
         </property>
